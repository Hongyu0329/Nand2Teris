S_0x127e04170 .scope module, "counter_tb" "counter_tb" 2 4;
 .timescale -9 -12;
v0x127e147d0_0 .var "clk", 0 0;
v0x127e14870_0 .net "count", 3 0, v0x127e14620_0;  1 drivers
v0x127e14920_0 .var "rst", 0 0;
S_0x127e042f0 .scope module, "uut" "counter" 2 10, 3 2 0, S_0x127e04170;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x127e04570_0 .net "clk", 0 0, v0x127e147d0_0;  1 drivers
v0x127e14620_0 .var "count", 3 0;
v0x127e146d0_0 .net "rst", 0 0, v0x127e14920_0;  1 drivers
E_0x127e04510 .event posedge, v0x127e146d0_0, v0x127e04570_0;
    .scope S_0x127e042f0;
T_0 ;
    %wait E_0x127e04510;
    %load/vec4 v0x127e146d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x127e14620_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x127e14620_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x127e14620_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x127e04170;
T_1 ;
    %vpi_call 2 17 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x127e04170 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x127e147d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x127e14920_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x127e14920_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x127e04170;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x127e147d0_0;
    %inv;
    %store/vec4 v0x127e147d0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
